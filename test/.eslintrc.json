{
  "env": {
    "mocha": true
  },
  "globals": {
    "AccessibilityLinter": false,
    "afterAll": false,
    "afterTimeout": false,
    "appendToBody": false,
    "beforeAll": false,
    "buildHtml": false,
    "clean": false,
    "domCleaner": false,
    "expect": false,
    "Mocha": false,
    "proxy": false,
    "requireModule": false,
    "requireScript": false,
    "requireTests": false,
    "ruleSpecs": false,
    "TestLogger": false,
    "uniqueId": false,
    "whenDomUpdates": false
  },
  "rules": {
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "class-methods-use-this": 0
  }
}
