{
  "env": {
    "mocha": true
  },
  "globals": {
    "expect": false,
    "$": false,
    "when": false,
    "AccessibilityLinter": false,
    "TestLogger": false,
    "ruleSpecs": false,
    "appendToBody": false,
    "domCleaner": false,
    "build": false,
    "clean": false,
    "uniqueId": false
  },
  "rules": {
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0
  }
}
