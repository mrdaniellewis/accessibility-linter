{
  "name": "accessibility-linter",
  "version": "1.14.0",
  "description": "Accessibility test a web page or web application",
  "main": "lib/index.js",
  "scripts": {
    "create-rule": "cp -r lib/rules/example",
    "lint": "eslint . --ext .js",
    "build-docs": "webpack --config webpack.docs.js && node .temp/generate > build/rules.md",
    "build": "npm run lint && webpack && npm run build-docs",
    "start": "webpack-dev-server --config webpack.dev.js"
  },
  "author": "Daniel Lewis",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.18",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^5.0.1",
    "mocha-browser-helpers": "mrdaniellewis/mocha-browser-helpers#v1.4.0",
    "mustache": "^2.3.0",
    "raw-loader": "^0.5.1",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "aria-config": "mrdaniellewis/aria-config#v1.2.0",
    "aria-extensions": "mrdaniellewis/aria-extensions#v1.3.0"
  },
  "docUrl": "https://mrdaniellewis.github.io/accessibility-linter/rules.md"
}
